# LiteLLM Router Environment Variables
# Copy this file to .env and add your API keys

# Premium Tier API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Standard Tier API Keys
GOOGLE_API_KEY=your_google_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here
TOGETHER_API_KEY=your_together_api_key_here

# Specialist Tier API Keys
GROQ_API_KEY=your_groq_api_key_here

# Redis Configuration (for caching)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Database (for usage tracking)
DATABASE_URL=postgresql://user:password@localhost:5432/litellm

# LiteLLM Configuration
LITELLM_MASTER_KEY=your_secure_master_key_here
LITELLM_PORT=4000
LITELLM_LOG_LEVEL=INFO

# Monitoring (optional)
OTEL_ENDPOINT=http://localhost:4317
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=https://cloud.langfuse.com

# Security
ALLOWED_IPS=
BLOCKED_IPS=
ENFORCE_HTTPS=false

# Cost Management
DAILY_BUDGET_USD=100
ALERT_THRESHOLD_USD=80

# Feature Flags
ENABLE_SEMANTIC_CACHE=true
ENABLE_BATCH_PROCESSING=true
ENABLE_PROGRESSIVE_COMPLEXITY=true
PREFER_FREE_MODELS=true
